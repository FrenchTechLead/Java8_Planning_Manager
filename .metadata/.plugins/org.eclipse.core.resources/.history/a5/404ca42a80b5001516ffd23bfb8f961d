package interface_Graphique.prof_mvc;

import java.awt.BorderLayout;

import javax.swing.JButton;
import javax.swing.JFrame;

public class ProfView {
	private JFrame frame;
	private JButton button1, button2, button3, button4, button5;

	public ProfView() {
		frame = new JFrame("ADE V9999 - Welcome Prof");
		frame.getContentPane().setLayout(new BorderLayout());
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.setSize(800, 600);
		frame.setResizable(false);
		frame.setVisible(true);
		frame.setLayout(null);

		button1 = new JButton("Consulter Emploi du temps");
		button1.setBounds(225, 50, 350, 30);
		frame.getContentPane().add(button1);
		
		button2 = new JButton("Consulter Emploi du temps");
		button2.setBounds(225, 50, 350, 30);
		frame.getContentPane().add(button2);
	}

	
	
	
	
	
	
	//getters and setters
	public JFrame getFrame() {
		return frame;
	}

	public JButton getButton1() {
		return button1;
	}

	public JButton getButton2() {
		return button2;
	}

	public JButton getButton3() {
		return button3;
	}

	public JButton getButton4() {
		return button4;
	}

	public JButton getButton5() {
		return button5;
	}

}
